{
  "name": "btc-price-guessing-game-backend",
  "description": "Backend for the bitcoin price guessing game",
  "version": "0.0.0",
  "private": true,
  "author": "Tomi Turtiainen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomi/btc-price-guessing-game.git"
  },
  "scripts": {
    "build-api-specs": "zx scripts/buildApiSpecs.mjs",
    "watch-api-specs": "zx scripts/buildApiSpecs.mjs --watch",
    "build-handlers": "zx scripts/buildHandlers.mjs",
    "watch-handlers": "zx scripts/buildHandlers.mjs --watch",
    "build": "npm run build-api-specs && npm run build-handlers",
    "ts": "tsc --noEmit",
    "watch-ts": "tsc -w --noEmit --preserveWatchOutput",
    "dev": "concurrently -k -p '[{name}]' -n 'handlers,apiSpecs,typescript' -c 'yellow.bold,blue.bold,cyan.bold' 'npm:watch-handlers' 'npm:watch-api-specs' npm:watch-ts",
    "deploy": "sam deploy"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.216.0",
    "@aws-sdk/client-secrets-manager": "3.216.0",
    "@aws-sdk/lib-dynamodb": "3.216.0",
    "@aws-sdk/util-dynamodb": "3.216.0",
    "@confconf/aws-secrets-manager": "2.1.0",
    "@confconf/confconf": "2.2.1",
    "date-fns": "2.29.3",
    "fromfrom": "1.8.0",
    "jsonwebtoken": "8.5.1",
    "lodash.chunk": "4.2.0",
    "node-fetch": "2.6.7",
    "openapi-backend": "5.6.0",
    "source-map-support": "0.5.21",
    "uuid": "9.0.0",
    "zx": "7.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.109",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash.chunk": "4.2.7",
    "@types/node": "18.11.9",
    "@types/uuid": "8.3.4",
    "chokidar-cli": "3.0.0",
    "concurrently": "7.6.0",
    "esbuild": "0.15.15",
    "typescript": "4.9.3"
  }
}
